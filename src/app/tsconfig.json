{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "../../dist",
    "baseUrl": "./app",
    "target": "es2015",
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "inlineSources": true,
    "noEmitHelpers": false,
    "noImplicitAny": false,
    "declaration": true,
    "skipLibCheck": false,
    "stripInternal": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "lib": [
      "es2018",
      "dom"
    ],
    "typeRoots": [
      "../../node_modules/@types",
      "../../node_modules/@types/pdfmake"
    ]
  },
  "exclude": [
    "**/node_modules/*"
  ],
  "angularCompilerOptions": {
    "genDir": "../../temp/factories",
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true,
    "entryModule": "app/pdfmake/pdfmake.module#PdfmakeModule"
  }
}
